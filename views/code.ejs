<nav class="navbar navbar-expand-lg navbar-light bg-light">
  <div class="container-fluid">
    <a class="navbar-brand" href="#"><%=label%></a>
    <form class="d-flex">
      <%actions.forEach((action) => {%>
        <button data-action="<%=action.jsFunction%>" class="btn btn-outline-success" type="submit"><%=action.label%></button>
      <%})%>
    </form>
  </div>
</nav>
<div class="row">
  <div class="col">
    <div id="editor"><%- editor_source -%></div>
  </div>
  <div class="col">
    <pre id="output"></pre>
  </div>
</div>
<script type="text/javascript">
  const editor = ace.edit('editor');
  editor.session.setMode('ace/mode/<%=ace.mode%>');
</script>
