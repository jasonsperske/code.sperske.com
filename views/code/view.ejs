<div class="grid-container">
  <div class="nav">
    <a class="navbar-brand" href="#"><%=label%></a>
    <form class="d-flex">
      <%actions.forEach((action) => {%>
        <button data-action="<%=action.jsFunction%>" class="btn btn-outline-success" type="submit"><%=action.label%></button>
      <%})%>
    </form>
  </div>
  <div class="footer"></div>
  <div class="content">
    <div class="tree"></div>
    <div class="code_pane">
      <div class="code"><div id="editor"><%- editor_source -%></div></div>
      <div class="output"><iframe id="output"></iframe></div>
    </div>
  </div>`
</div>
<script type="text/javascript">
  const editor = ace.edit('editor');
  editor.session.setMode('ace/mode/<%=ace.mode%>');
</script>

<script type="text/javascript" src="/static/js/lang/<%=href%>.min.js"></script>
